{
  "buildCommand": "cd web-frontend && npm install && npm run build",
  "outputDirectory": "web-frontend/dist",
  "installCommand": "npm install --prefix web-backend",
  "devCommand": "cd web-frontend && npm run dev",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
